<%- if controller_name != 'sessions' %>
  <%= link_to ">ログインはこちら<", new_session_path(resource_name), class: "font-weight-bold custom-link text-dark" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to ">新規登録はこちら<", new_registration_path(resource_name), class: "font-weight-bold text-right custom-link text-dark" %><br />
<% end %>
