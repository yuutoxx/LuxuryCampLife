<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-3 mt-1'>
      <%= render 'form', post: @post %>
      <%= render 'tag', tag_list: @tag_list %>
    </div>
    <div class='col-md-6 p-0'>
      <%= render 'seach', q: @q %>
      <%= render 'index', posts: @posts %>
    </div>
    <div class='col-md-3'>
      <%= render 'choice', q: @q %>
    </div>
  </div>
</div>
